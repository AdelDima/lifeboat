{
  "name": "lifeboat",
  "version": "0.6.0",
  "author": "Josh Larson <jplhomer@gmail.com>",
  "description": "A Docker UI for beginners",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint":
      "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix":
      "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main":
      "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer":
      "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit",
    "unit":
      "NODE_ENV=test mocha-webpack --webpack-config .electron-vue/webpack.renderer.config.js --require test/setup.js test/**/*.spec.js",
    "watch":
      "NODE_ENV=test mocha-webpack --webpack-config .electron-vue/webpack.renderer.config.js --watch --require test/setup.js test/**/*.spec.js",
    "postinstall": "npm run lint:fix && npm run electron-rebuild"
  },
  "build": {
    "productName": "Lifeboat",
    "appId": "org.jplhomer.lifeboat",
    "directories": {
      "output": "build"
    },
    "files": ["dist/electron/**/*"],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "ansi_up": "^2.0.2",
    "axios": "^0.16.1",
    "dockerode": "^2.5.1",
    "electron-log": "^2.2.9",
    "electron-settings": "^3.1.2",
    "electron-updater": "^2.16.1",
    "fix-path": "^2.1.0",
    "js-yaml": "^3.10.0",
    "markdown": "^0.5.0",
    "mousetrap": "^1.6.1",
    "node-pty": "^0.7.3",
    "vue": "^2.5.8",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "which": "^1.3.0",
    "xterm": "Tyriar/xterm.js#vscode-release/1.18"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "bulma": "^0.6.0",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.6",
    "electron-builder": "^19.30.4",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-rebuild": "^1.6.0",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^21.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "jsdom": "^11.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.0.2",
    "mocha-webpack": "^1.0.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.7.0",
    "require-dir": "^0.3.0",
    "sass-loader": "^6.0.6",
    "shebang-loader": "^0.0.1",
    "sinon": "^4.1.2",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "vue-test-utils": "^1.0.0-beta.6",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
